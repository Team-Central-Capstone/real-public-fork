    "EvaluationTimeOnSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  },
  "Jwt": {
    "Firebase": {
      "ValidIssuer": "https://securetoken.google.com/real--dating",
      "ValidAudience": "real--dating"
    }
  }
}